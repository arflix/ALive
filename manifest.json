{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Colab Alive by BaleLio",
  "version": "1.2.1",
  "manifest_version": 2,
  "description": "Keep your Colab tabs alive with this nifty extension!",
  "default_locale": "en",
  "background": {
    "scripts": []
  },
  "content_scripts": [
    {
      "matches": [
        "https://colab.research.google.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}